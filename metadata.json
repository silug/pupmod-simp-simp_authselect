{
  "name": "simp-simp_authselect",
  "version": "0.1.0",
  "author": "simp",
  "summary": "This module wraps jcpunk/authselect and configures the pam authselect files in a way that is compatible with the simp/pam module.",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_authselect",
  "project_page": "https://github.com/simp/pupmod-simp-simp_authselect",
  "issues_url": "https://github.com/simp/pupmod-simp-simp_authselect/issues",
  "tags": [
    "simp",
    "simp_authselect"
  ],
  "dependencies": [
    { "name": "simp/pam"
    },
    { "name": "simp/compliance_markup"
    },
    { "name": "jcpunk/authselect"
    },
    { "name": "simp_options"
    },
    { "name": "oddjob"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.15.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.6.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ]
}
